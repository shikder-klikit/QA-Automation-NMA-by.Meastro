{
  "name": "qa-automation-nma-by.meastro",
  "version": "1.0.0",
  "description": "Maestro is built on learnings from its predecessors (Appium, Espresso, UIAutomator, XCTest, Selenium, Playwright) and allows you to easily define and test your Flows",
  "main": "index.js",
  "scripts": {
    "install meastro": "brew tap mobile-dev-inc/tap  \n  brew install maestro",
    "maestro studio": "maestro studio",
    "Connecting to Your Device": "maestro test",
    "login": "maestro test Klikit_Flows/Login.yaml",
    "homepage": "maestro test Klikit_Flows/homepage.yaml",
    "Add Order": "maestro test Klikit_Flows/Add_Order.yaml",
    "Record test": "maestro record --local runTest.yaml",
    "install allure": "npm install -g allure-commandline",
    "Report generate": "maestro test runTest.yaml --format junit --output ./maestro-results/results.xml",
    "Show Report": "allure serve ./maestro-results"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shikder-klikit/QA-Automation-NMA-by.Meastro.git"
  }
}